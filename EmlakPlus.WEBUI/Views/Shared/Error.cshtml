@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<section class="content">
    <div class="error-page">
        <h2 class="headline @Model.Css"> @Model.Code</h2>

        <div class="error-content">
            <h3><i class="fas fa-exclamation-triangle @Model.Css"></i> @Model.Title</h3>

            <p>
              @Model.Description
            </p>

          <a href="@Model.ReturnUrl" class="text-primary">Geri Dön</a>
        </div>
        <!-- /.error-content -->
    </div>
    <!-- /.error-page -->
</section>

